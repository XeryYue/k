{
  "name": "vite-plugin-compression2",
  "version": "0.9.3",
  "description": "a fast vite compression plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "bump",
    "dev": "bump -w",
    "test": "c8 -r=lcov ava __tests__/*.spec.ts",
    "lint": "eslint . --fix",
    "prepare:e2e": "cd e2e && npm run installer",
    "e2e": "ava e2e/**/*.spec.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nonzzz/vite-compression-plugin.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "compress"
  ],
  "author": "Kanno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-compression-plugin/issues"
  },
  "homepage": "https://github.com/nonzzz/vite-compression-plugin#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "eslint": "^8.40.0",
    "eslint-config-kagura": "^1.1.0",
    "no-bump": "^0.7.1",
    "playwright": "^1.32.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts",
      "e2e/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
